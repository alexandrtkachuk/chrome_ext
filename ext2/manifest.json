{
  "manifest_version": 2,

  "name": "Getting started2 it is my apppppppp",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["jquery.js", "mycode.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "http://192.168.56.100/",
    "http://localhost:8000/",
    "https://github.com/blog",
    "http://*/",
    "https://*/*",
    "http://*/*",
    "tabs"
  ]

}